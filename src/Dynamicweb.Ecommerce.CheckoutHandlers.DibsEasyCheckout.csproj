<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>2.1.2</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Description>DibsEasyCheckout</Description>
  </PropertyGroup>
  <PropertyGroup>
    <PackageProjectUrl>https://doc.dynamicweb.com/</PackageProjectUrl>
	<PackageTags>Dynamicweb dw10 addin ecommerce checkout</PackageTags>
    <Product>Dynamicweb CMS</Product>
    <Company>Dynamicweb Software A/S</Company>
    <Authors>Dynamicweb Software A/S</Authors>
    <Copyright>Copyright © 2020 Dynamicweb Software A/S</Copyright>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Dynamicweb" Version="9.10.31" />
    <PackageReference Include="Dynamicweb.Core" Version="3.1.0" />
    <PackageReference Include="Dynamicweb.Data" Version="3.0.2" />
    <PackageReference Include="Dynamicweb.Ecommerce" Version="1.10.90" />
    <PackageReference Include="Dynamicweb.Environment" Version="4.0.6" />
    <PackageReference Include="Dynamicweb.Extensibility" Version="4.0.2" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
</Project>